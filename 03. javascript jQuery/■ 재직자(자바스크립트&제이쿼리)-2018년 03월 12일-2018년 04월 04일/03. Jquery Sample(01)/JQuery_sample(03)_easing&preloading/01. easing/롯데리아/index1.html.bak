<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>롯데리아</title>
<style type="text/css">
#wrap {background: url("images/intro_bg.jpg") no-repeat;height: 930px;width: 100%;}
.intro_main{height: 930px; margin: 0 auto; position: relative;width: 1240px;}
.deco0{left: 350px; position: absolute;top: 155px;}
.deco1{left: 83px; opacity: 0;position: absolute;top: -200px;}
.deco2 { left: 235px;opacity: 0;position: absolute; top:-90px;}
.deco3 { left: 793px;opacity: 0;position: absolute;top:-500px;}
.deco4 {left: 920px;opacity: 0;position: absolute; top:-500px;}
.deco5 { left: 1018px;opacity: 0;position: absolute;top:-200px;}
.deco6 {left: 280px;  opacity: 0;position: absolute;top: 100px;}
.deco7 {left: 454px; opacity: 0;position: absolute;top: 720px;z-index: 100;}
.deco8 { left: -60px;opacity: 0;position: absolute;top: 280px;}
.deco9 { left: 1100px;opacity: 0;position: absolute;top:-500px;}
</style>
<script type="text/javascript" src="js/jquery-1.10.1.min.js"></script>
<script type="text/javascript" src="js/jquery.easing.1.3.js"></script>
<script type="text/javascript">
$(function(){

	$(".deco6").stop().delay(100).animate({opacity:1, top:232}, 1300,"easeOutBack");
	$(".deco0").stop().delay(1000).animate({opacity:1, top:55}, 1000,"easeOutQuad");
	$(".deco7").stop().delay(1300).animate({opacity:1}, 1000,"easeOutQuad");
 
	$(".deco1").stop().delay(2500).animate({opacity:1, top:0}, 1300, "easeOutBack");
	$(".deco2").stop().delay(3000).animate({opacity:1, top:0}, 800, "easeOutBack");
	$(".deco3").stop().delay(2000).animate({opacity:1, top:0}, 1000, "easeOutBack");
	$(".deco4").stop().delay(3200).animate({opacity:1, top:0}, 1300, "easeOutBack");
	$(".deco9").stop().delay(3200).animate({opacity:1, top:0}, 1500, "easeOutBack");
	$(".deco5").stop().delay(2500).animate({opacity:1, top:0}, 1000, "easeOutBack");				
	$(".deco8").stop().delay(100).animate({opacity:1}, 1000,"easeOutQuad");
			

});
</script>
</head>

<body>
<div id="wrap">
<div class="intro_main">
  <div class="deco7"><a href="#"><img src="images/intro_btn.png" /></a></div>
  <div class="deco0"><img src="images/intro_logo.png" /></div>
  <div class="deco1"><img src="images/intro_img01.png" /></div>
  <div class="deco2"><img src="images/intro_img02.png" /></div>
  <div class="deco3"><img src="images/intro_img06.png" /></div>
  <div class="deco4"><img src="images/intro_img05.png" /></div>
  <div class="deco5"><img src="images/intro_btn_facebook.png" /></div>
  <div class="deco6"><img src="images/intro_merry.png" /></div>
  <div class="deco9"><img src="images/intro_img04.png" /></div>
  <div class="deco8"><img src="images/intro_img07.png" /></div>
</div>
</div>
</body>
</html>
